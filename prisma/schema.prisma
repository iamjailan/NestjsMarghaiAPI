generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id              String    @id @default(uuid())
  user_name       String
  last_name       String
  age             Int?
  dob             DateTime?
  bio             String?
  city            String?
  country         String?
  address         String?
  profile_picture String?
  gender          String?
  phone_number    String?
  email           String    @unique
  password        String
}
